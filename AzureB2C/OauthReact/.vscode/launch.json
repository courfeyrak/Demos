    "version": "0.2.0",
    "configurations": [
        {
            "expo": {
              "slug": "MyApp", // Project slug
              "name": "MyApp", // Project name
              "sdkVersion": "31.0.0", // Expo SDK version
              "entryPoint": ".vscode\\exponentIndex.js" // Entrypoint for the project
            },
            "name": "MyApp" // Project name
        },
        {
            "name": "Debug Android",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android"
        },
        {
            "name": "Run Android",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "enableDebug": false
        },
        {
            "name": "Debug iOS",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios"
        },
        {
            "name": "Run iOS",
            "cwd": "${workspaceFolder}",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "enableDebug": false
        }
    ]
}